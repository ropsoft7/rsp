<?xml version="1.0"?>
<compify_config>
  <windowSwitcher show="no" preview="yes" outlines="no" allWorkspaces="no">
</windowSwitcher>
  <keyboard>
    <default/>
    <keybind key="A-Space">
      <action name="Execute" command="/usr/lib/rsp/toggle/spotrun-toggle"/>
    </keybind>
    <keybind key="A-Delete">
      <action name="Execute" command="/usr/local/bin/rsp-leave"/>
    </keybind>
    <keybind key="C-A-Delete">
      <action name="Execute" command="/usr/local/bin/qps"/>
    </keybind>
    <keybind key="Print">
      <action name="Execute" command="/usr/local/bin/rsp.printScreen"/>
    </keybind>
    <keybind key="A-Return">
      <action name="Execute" command="/usr/local/bin/qterminal --drop"/>
    </keybind>
    <keybind key="Escape">
      <action name="Execute" command="pkill qps"/>
      <action name="Execute" command="pkill winskip"/>
      <action name="Execute" command="pkill spotrun"/>
      <action name="Execute" command="pkill rsp-leave"/>
      <action name="Execute" command="pkill -9 rsp-config*"/>
      <action name="Execute" command="pkill -f 'qterminal --drop'"/>
    </keybind>
  </keyboard>

  <theme>
    <cornerRadius>5</cornerRadius>
    <name>RspAdwaitaDark</name>
    <dropShadows>yes</dropShadows>
  </theme>
  <placement>
    <policy>Cascade</policy>
  </placement>
  <libinput>
    <device>
      <naturalScroll>no</naturalScroll>
    </device>
  </libinput>
</compify_config>
